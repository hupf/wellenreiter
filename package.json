{
  "name": "wellenreiter",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wellenreiterapp.com",
  "dependencies": {
    "auth0-js": "^9.8.1",
    "aws-amplify": "^1.1.6",
    "howler": "^2.0.15",
    "is-url": "^1.2.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-notifications": "^1.4.3",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "1.16.4",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "env-cmd .env.production.local react-scripts start",
    "build": "env-cmd .env.production react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write \"./**/*.{js,ts,json,css,scss,html,md,yaml}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,scss,html,md,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
